package com.rays.numberseries;

public class ArmstrongNumber {
	public static void main(String[] args) {

		int a = 373;
		int b = 0;
		int c;
		int d = a;

		while (d > 0) {
			c = d % 10;
			b = b + (c * c * c);
			d = d / 10;

		}

		if (b == a) {
			System.out.println("this is an Armstrong Number");
		} else {
			System.out.println("this is not an Armstrong Number");
		}
	}

}
