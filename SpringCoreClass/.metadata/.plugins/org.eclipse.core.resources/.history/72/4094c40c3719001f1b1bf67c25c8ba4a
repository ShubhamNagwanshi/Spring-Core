package com.rays.numberseries;

public class PrimeNumber {
public static void main(String[] args) {
	
	int num = 8;
	int count = 0;
	
	for (int i = 2; i < num; i++) {
		
		if(num%i == 0) {
			
			count++;
		}
		
	}
	if(count == 0) {
		System.out.println("this is a prime number");
	}else {
		System.out.println("this is not a prime number");
	}
}
}
